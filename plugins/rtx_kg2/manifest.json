{
    "version": "0.2",
    "requires": ["jsonlines"],
    "dumper": {
        "data_url": [
            "http://su06:8080/dataupload/edges.jsonl",
            "http://su06:8080/dataupload/nodes.jsonl"
        ],
        "uncompress": false,
        "release": "version:get_release",
        "schedule": "0 0 * * *"
    },
    "uploaders": [
        {
            "name" : "rtx_kg2_edges_merged",
            "parser": "hub.dataload.data_parsers:load_merged_edges",
            "on_duplicates": "merge",
            "mapping" : "mapping:merged_edges_mapping"
        }
    ]
}
